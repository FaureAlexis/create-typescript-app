{
    "useTaobaoRegistry": false,
    "packageManager": "npm",
    "presets": {
        "ts": {
            "useConfigFiles": true,
            "plugins": {
                "@vue/cli-plugin-babel": {},
                "@vue/cli-plugin-typescript": {
                "classComponent": false,
                "useTsWithBabel": true
                },
                "@vue/cli-plugin-router": {
                "historyMode": true
                },
                "@vue/cli-plugin-vuex": {},
                "@vue/cli-plugin-eslint": {
                "config": "airbnb",
                "lintOn": [
                    "save"
                ]
                },
                "@vue/cli-plugin-unit-jest": {}
            },
            "vueVersion": "3"
            }
    },
    "latestVersion": "5.0.8"
}
